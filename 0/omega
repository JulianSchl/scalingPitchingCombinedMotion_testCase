FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];


internalField   uniform 0.121;

boundaryField
{
    "Border|Inlet|Outlet"
	{
        type            inletOutlet;
        inletValue      uniform 0.121;
        value           uniform 0.121;
	}
	
	Hole1
	{
        type            omegaWallFunction;
        value           uniform 0.121;
	}


	Hole2
	{
        type            omegaWallFunction;
        value           uniform 0.121;
	}
    BaseAndTop
    {
        type            empty;
    }
}


